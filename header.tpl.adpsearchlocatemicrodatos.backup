	{if !isset($content_only) || !$content_only}
		{if isset($restricted_country_mode) && $restricted_country_mode}
			<div id="restricted-country">
				<p>{l s='You cannot place a new order from your country.'}{if isset($geolocation_country) && $geolocation_country} <span class="bold">{$geolocation_country|escape:'html':'UTF-8'}</span>{/if}</p>
			</div>
		{/if}
		<div id="page">
			<div class="header-container">
				<header id="header">
					{include file="$tpl_dir./covid-19.tpl"} {*MM Just comment this line to quit banner. The style are inside tpl*}
					{capture name='displayBanner'}{hook h='displayBanner'}{/capture}
					{if $smarty.capture.displayBanner}
						<div class="banner">
							<div class="container">
								<div class="row">
									{$smarty.capture.displayBanner}
								</div>
							</div>
						</div>
					{/if}
					{capture name='displayNav'}{hook h='displayNav'}{/capture}
					{if $smarty.capture.displayNav}
						<div class="nav">
							<div class="container">
								<div class=" flex">
									<div class="linksleft"><a href="{$link->getPageLink('contact', true)|escape:'html':'UTF-8'}" class="Contact"><span class="icon"></span> {if $lang_iso == en}Contact Us{/if}{if $lang_iso == fr}Contactez-nous{/if}</a>
									<span class="store"><a href="https://www.google.ca/maps/place/Massageboutik.com/@45.5632882,-73.7075398,14z/data=!4m5!3m4!1s0x0:0x4fcea7e2fd88376e!8m2!3d45.5696275!4d-73.6914465?hl={$lang_iso}">{l s="Visit our store"}</a></span></div>
									<nav>{$smarty.capture.displayNav}</nav>
								</div>
							</div>
						</div>
					{/if}
					<div>
						<div class="container">
							<div class="row">
								<div id="header_logo" class="col-sm-5">
									<a href="{if isset($force_ssl) && $force_ssl}{$base_dir_ssl}{else}{$base_dir}{/if}" title="{$shop_name|escape:'html':'UTF-8'}">
										<img class="logo img-responsive" src="{$logo_url}" alt="{$shop_name|escape:'html':'UTF-8'}"{if isset($logo_image_width) && $logo_image_width} width="{$logo_image_width}"{/if}{if isset($logo_image_height) && $logo_image_height} height="{$logo_image_height}"{/if}/>
									</a>
								</div>
								<div class="col-sm-7 info">
									<div class="col-sm-7 freeship">
										<span class="firstline"><img src="{$img_dir}truck.svg" alt="Truck" width="31" height="18" class="icon"><a href="{$link->getCMSLink('1')|escape:'html':'UTF-8'}">{l s="Free Shipping"}</a></span> <span class="secondline cursive">{l s="on orders over"} <span class="important">{if $lang_iso == en}${/if}349.99{if $lang_iso == fr}${/if}</span> {l s="in Canada"}*</span>
									</div>
									<div class="getadvice col-sm-5">
										<div class="firstline"><a href="{$link->getPageLink('contact', true)|escape:'html':'UTF-8'}">{l s="GET ADVICE FROM EXPERTS"}</a></div> 
										<span class="secondline">
										<span class="phone"><a href="tel:4509671970">(450) 967-1970</a></span>
											<span class="phone"><a href="tel:1-866-931-0920">1-866-931-0920</a></span> 
											
											</span>
									</div>
								</div>
								{if isset($HOOK_TOP)}{$HOOK_TOP}{/if}
							</div>
						</div>
					</div>
				</header>
			</div>
			<div id="menucontainer" >
				<div class="container">
					<div class="row">
					{hook h='displayMM'}
					</div>
				</div>
			</div>
			<div class="columns-container">

				
				{assign var=CMSids value=[6]} {*Full width for the following ids CMS*}

				{if $cms->id|in_array:$CMSids} {*Remove container class for more freedom to design inside CMS section*}
				<div id="columns" class="CMScontainer">
					{if $page_name !='index' && $page_name !='pagenotfound'}
						<div class="container">
						{include file="$tpl_dir./breadcrumb.tpl"}
						</div>
					{/if}
				{else}
				<div id="columns" class="container">
					{if $page_name !='index' && $page_name !='pagenotfound'}
						{include file="$tpl_dir./breadcrumb.tpl"}
					{/if}
				{/if}
					
					<div id="slider_row" class="row">
						{capture name='displayTopColumn'}{hook h='displayTopColumn'}{/capture}
						{if $smarty.capture.displayTopColumn}
							<div id="top_column" class="center_column col-xs-12 col-sm-12">{$smarty.capture.displayTopColumn}</div>
						{/if}
					</div>
					<div class="row">
						{if isset($left_column_size) && !empty($left_column_size)}
						<div id="left_column" class="column col-xs-12 col-sm-{$left_column_size|intval}">{$HOOK_LEFT_COLUMN}</div>
						{/if}
						{if isset($left_column_size) && isset($right_column_size)}{assign var='cols' value=(12 - $left_column_size - $right_column_size)}{else}{assign var='cols' value=12}{/if}
						<div id="center_column" class="center_column col-xs-12 col-sm-{$cols|intval}">
	{/if}
